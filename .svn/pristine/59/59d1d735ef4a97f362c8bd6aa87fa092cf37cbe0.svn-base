			<timeTable>
				<period>
					<second>30</second>
				</period>
			</timeTable>
			<return messageType="message type">
				<sentTrigger class="full class name">
				</sentTrigger>
				<switchTo service="service name" marshalClass="full class name">
				</switchTo>
			</return>
		</cronjob>
	</sessionCronjobs>

	<nosessionCronjobs>
